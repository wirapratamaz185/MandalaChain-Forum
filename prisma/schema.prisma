// schema.prisma
generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String      @id @default(uuid()) @db.Uuid
}

model Community {
  id               String             @id @default(uuid()) @db.Uuid
  creatorId        String          
  createdAt        DateTime           @default(now())
  numberOfMembers  Int
  privacyType      String
}

model CommunitySnippet {
  id          String    @id @default(uuid()) @db.Uuid
  communityId String    @db.Uuid
  isAdmin     Boolean
}
